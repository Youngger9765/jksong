<div class="container">
  <div class="row">
    <% @issues.each do |i| %>
      <div class="col-md-3" id="issue_board">
            <h3><%= link_to i.name, issue_path(i) %></h3>
            <div id="issue_photo">
              <% if i.logo.exists? %>
                <%= link_to image_tag(i.logo.url(:original)),issue_path(i)%>
              <%end%>
            </div>
      </div>
    <% end %>
  </div>
</div>


<%= paginate @issues %>

